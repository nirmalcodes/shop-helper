rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {

    match /messages/{allPaths=**} {
      allow read: if request.auth != null;
      allow write, delete: if equest.auth != null && firestore.get(/databases/(default)/documents/users/$(request.auth.uid)).data.role in ['root', 'admin'];
    }

  }
}